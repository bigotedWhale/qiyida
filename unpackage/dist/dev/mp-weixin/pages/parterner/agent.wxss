
.wrap {
  padding: 12px 0;
  min-height: calc(100vh - 24px);
  background-color: #fff;
}
.title {
  font-size: 28rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 24rpx;
  color: #333;
}
input {
  height: 80rpx;
  background-color: #fff;
  padding:0 24rpx;
  color: #333;
}
.form-item {
  padding: 24rpx 0;
  padding-bottom: 12rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f1f1f1;
  margin: 0 24rpx;
}

